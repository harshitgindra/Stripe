        <td>
            @Html.DisplayFor(modelItem => item.event_Away_Team_Score)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.event_School_Field_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.School.sch_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.School1.sch_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sport_Name.spt_Name)
        </td>
        <td>
           
        </td>
    </tr>
}

</table>
